[localhost]
localhost	ansible_connection=ssh	ansible_ssh_user=bruno

[rpm-build-server]
rpmbuilder-centos.apse1.uat.maestrano.io	ansible_connection=ssh	ansible_ssh_user=root ansible_ssh_private_key_file=/home/bruno/mno-keys/MaestranoUAT/global-dev-apse1.pem

[rpm-repository-uat]
puppet.apse1.uat.maestrano.io	ansible_connection=ssh	ansible_ssh_user=ec2-user ansible_ssh_private_key_file=/home/bruno/mno-keys/MaestranoUAT/global-dev-apse1.pem
